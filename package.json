{
  "name": "multi-package-chatbot",
  "version": "1.0.0",
  "description": "Advanced multi-package data collection chatbot with NLP and real-time validation",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/index.ts",
    "dev:web": "ts-node src/server.ts",
    "build": "tsc && npm run copy-public",
    "copy-public": "node -e \"require('fs').cpSync('public', 'dist/public', {recursive: true})\"",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "chatbot",
    "package",
    "data-collection",
    "nlp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "compromise": "^14.11.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "inquirer": "^8.2.5",
    "joi": "^17.11.0",
    "natural": "^6.10.0",
    "ora": "^5.4.1",
    "readline-sync": "^1.4.10",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.11",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.16",
    "@types/readline-sync": "^1.4.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "multer": "^2.0.2",
    "papaparse": "^5.5.3",
    "pdf-parse": "^2.4.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
